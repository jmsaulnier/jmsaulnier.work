    "normalize.css": "^5.0.0",
    "preact": "^7.2.0",
    "preact-compat": "^3.14.3",
    "promise": "^7.1.1",
    "proptypes": "^0.14.4",
    "three": "^0.85.1",
    "three-orbit-controls": "^82.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-latest": "^6.24.0",
