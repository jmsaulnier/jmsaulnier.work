    "glslify-loader": "^1.0.2",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.15.0",
    "sw-precache-webpack-plugin": "^0.9.1",
