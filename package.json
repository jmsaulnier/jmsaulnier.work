  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "start": "http-server ./build --gzip --cors -c-1 --ssl --cert localhost.pem --key localhost.pem",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "prebuild": "del build && mkdirp build && ncp static build",
    "cname": "[[ -e CNAME ]] && cp CNAME build/CNAME || echo 'no CNAME found'",
    "deploy": "npm run build && npm run cname && gh-pages -d build"
  },
  "dependencies": {
    "domready": "^1.0.8",
    "glsl-blend-soft-light": "^1.0.5",
