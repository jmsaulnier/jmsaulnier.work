  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "start": "superstatic build -p 8080 --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "prebuild": "del build && mkdirp build && ncp static build"
  },
  "dependencies": {
    "normalize.css": "^5.0.0",
    "preact": "^7.1.0",
    "preact-compat": "^3.9.4",
    "preact-redux": "^1.2.0",
    "preact-router": "^2.3.2",
    "proptypes": "^0.14.3",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "compression-webpack-plugin": "^0.3.2",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "del-cli": "^0.2.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "html-webpack-plugin": "^2.26.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.2.1",
    "raw-loader": "^0.5.1",
    "script-ext-html-webpack-plugin": "^1.4.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "superstatic": "^4.0.3",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0-rc.3",
    "webpack-dev-server": "2.2.0-rc.0"
  }
